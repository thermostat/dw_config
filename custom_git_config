###########################################################################
# Git config
###########################################################################

[core]
  editor = emacs -nw

[push]
  default = simple

[alias]
  out = log --pretty=format:\"%C(auto)%h %aE %C(auto)%ai %s\" --graph @{u}..  
  stat = status --short
  shlog = log  --pretty=format:\"%C(auto)%h %aE %C(auto)%ai %s\"
